<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUIDMSMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
xmlns:views="clr-namespace:MAUIDMSMobile.Mvvm.Views"
     FlyoutBehavior="Flyout"
    Title="MAUIDMSMobile">
    <FlyoutItem Title="USER" FlyoutDisplayOptions="AsMultipleItems"
                Icon="user.png">

        <ShellContent Route="Homepage" Title="Accueil">
            <views:Homepage />
        </ShellContent>
        <ShellContent Route="Login" Title="login" >
            <views:LoginPage />
        </ShellContent>

    </FlyoutItem>

</Shell>
